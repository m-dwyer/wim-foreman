<%#
kind: PXELinux
name: WIM default PXELinux
%>
<% boot_files_uris = @host.operatingsystem.boot_files_uri(@host.medium,@host.architecture) %>
DEFAULT wimbootpe
LABEL wimbootpe
     com32 linux.c32
     append <%= @host.operatingsystem.bootfile(@host.arch, :kernel) %> initrdfile=<%= @host.operatingsystem.bootfile(@host.arch,:bcd) %>,<%= @host.operatingsystem.bootfile(@host.arch,:bootsdi) %>,<%= @host.operatingsystem.bootfile(@host.arch,:bootwim) %>
